pipeline{
	agent any

	Stages {

		stage ('Compile'){
			steps{

				withMaven (maven : 'maven_3.5.3'){

					sh 'mvn -B -DskipTests clean package

				}
			}

		}

		stage ('Testing'){
			steps{

				withMaven (maven : 'maven_3.5.3'){

					sh 'mvn test'

				}
			}

		}

		stage ('Delivert'){
			steps{

				withMaven (maven : 'maven_3.5.3'){

					sh './jenkins/scripts/deliver.sh'

				}
			}

		}

	}

}